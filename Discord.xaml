<Window x:Class="Assignment_2.Discord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment_2"
        mc:Ignorable="d"
        Title="Discord message" Height="352" Width="554">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="newbackground.jpg"/>
        </Grid.Background>
        <Button Content="Send" Style="{StaticResource ButtonStyle}" HorizontalAlignment="Left" Margin="228,274,0,0" VerticalAlignment="Top" Width="76" Click="Button_Click" Height="24"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="123" Margin="56,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="427"/>
        <TextBox x:Name="nameTextBox" HorizontalAlignment="Left" Height="25" Margin="56,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="427"/>
        <Label Content="Name or username" HorizontalAlignment="Left" Margin="56,37,0,0" VerticalAlignment="Top" Width="114" Height="25"/>
        <Label Content="Send message to collaborator channel" HorizontalAlignment="Left" Margin="56,115,0,0" VerticalAlignment="Top" Width="300" Height="26"/>

    </Grid>
</Window>
